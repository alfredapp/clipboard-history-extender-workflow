				<string>readonly tmp_db="$(mktemp)"
trap '/bin/rm "${tmp_db}"' EXIT

/bin/cp "${HOME}/Library/Application Support/Alfred/Databases/clipboard.alfdb" "${tmp_db}"

/usr/bin/sqlite3 \
  "${alfred_workflow_data}/clipboard.alfdb" \
  "ATTACH database '${tmp_db}' AS source;
   INSERT INTO clipboard SELECT item, ts, apppath FROM source.clipboard WHERE dataType IS 0;
   DETACH DATABASE source;
   DELETE FROM clipboard WHERE rowid NOT IN (
     SELECT MAX(rowid) 
     FROM clipboard
     GROUP BY item
   );"</string>
